{
  "version": 2,
  "headers": [
    {
      "key": "Access-Control-Allow-Origin",
      "value": "http://localhost:3000"
    },
    {
      "key": "Access-Control-Allow-Credentials",
      "value": "true"
    },
    {
      "key": "Access-Control-Allow-Headers",
      "value": "X-Requested-With, Access-Control-Allow-Origin, X-HTTP-Method-Override, Content-Type, Cache-Control, Authorization, Accept"
    },
    {
      "key": "Access-Control-Max-Age",
      "value": "86400"
    }
  ],
  "builds": [
    {
      "src": "/api/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
